{
    "env": {
      "myDefaultIncludePath": ["${workspaceFolder}"],
      "myCompilerPath": "/usr/bin/clang++",
      "myIncludePath": "/usr/local/Cellar/python@3.8/3.8.3/Frameworks/Python.framework/Versions/Current/include/python3.8"
    },
    "configurations": [
      {
        "name": "Mac",
        "intelliSenseMode": "clang-x64",
        "includePath": ["${myDefaultIncludePath}","${myIncludePath}"],
        "macFrameworkPath": ["/System/Library/Frameworks"],
        "compilerPath": "${myCompilerPath}",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "browse": {
          "path": ["${workspaceFolder}"],
          "limitSymbolsToIncludedHeaders": true,
          "databaseFilename": ""
        }
      }
    ],
    "version": 4
  }